{% extends 'base.html.twig' %}

{% block title %}Hello BienvenueController!{% endblock %}

{% block body %}


    <h1>Bienvenues aux personnes ci-dessous : </h1>
    <ul>
        {% for personne in personnes %}
            <li><a href=" {{ path( 'app_bienvenue_prenom', {"prenom" : personne } ) }}"> {{ personne | capitalize }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}